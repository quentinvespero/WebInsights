{
    "manifest_version": 3,
    "name": "WebInsights",
    "version": "0.9",
    "description": "A chrome extension to get insights about webpages",
    "action": {
        "default_popup": "index.html",
        "default_icon": "./assets/icons/icon.png",
        "default_title": "WebInsights"
    },
    "icons": {
        "32":"./assets/icons/icon.png",
        "128":"./assets/icons/icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "contextMenus",
        "tabs"
    ],
    "background":{
        "service_worker" : "background.js"
    }
}